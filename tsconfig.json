{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "isolatedModules": true,
    "removeComments": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content*"],
      "@styles/*": ["./src/styles/*"],
      "@pages/*": ["./src/pages/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@data/*": ["./src/data/*"],
      "@config/*": ["./src/config/*"]
    },
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "dist",
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
